{
  "entrypoint": "index.html",
  "shell": "src/ft-connect-front-end-sdk-tool.html",
  "fragments": [
  ],
  "sources": [
    "src/**/*",
    "manifest/**",
    "robots.txt"
  ],
  "extraDependencies": [
    "bower_components/ace-builds/src-noconflict/mode-html.js",
    "bower_components/ace-builds/src-noconflict/theme-chrome.js",
    "manifest.json"
  ],
  "builds": [
    {
      "name": "prod",
      "preset": "es5-bundled"
    },
    {
      "name": "debug",
      "preset": "es5-bundled",
      "bundle": true,
      "js": { "minify": false},
      "css": {"minify": false},
      "html": { "minify": false},
      "addServiceWorker": false
    },
    {
      "name": "dev",
      "bundle": false,
      "js": {"minify": false, "compile": false},
      "css": {"minify": false},
      "html": {"minify": false},
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ]
}
